import java.util.*;
class maini{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        double  s=0;
        int d=0;
        s=3.14*n*n;
        if(s-(int)s>0.5){
            s=s+0.5;
    
        System.out.println(s);
        }else{
            d=(int)s;
            System.out.println(d);
        }
        
 }
}
