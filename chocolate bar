import java.util.*;
class mini{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     int b=0;
     int a[]=new int[n];
     for(int i=0;i<n;i++){
         a[i]=sc.nextInt();
     }
     for(int i=0;i<n;i++){
         b=b+((a[i]+2)/3);
     }
       System.out.println(b);
    }
} 
