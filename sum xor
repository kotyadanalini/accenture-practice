import java.util.*;
class Mddin{
    public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    int arr[]=new int[n];
    for(int i=0;i<n;i++){
        arr[i]=sc.nextInt();
    }
    int c=0;
    int d=0;
    
    for(int i=0;i<n;i++){
        if(arr[i]%2==0){
            c=c^arr[i];
        }else{
            d=d+arr[i];
        }
    }
    System.out.println(c-d);
    
}
}
