import java.util.*;
class mini{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
     int n=sc.nextInt();
     int c=0;
     int d[]=null;
     int a[]=new int[n];
     for(int i=0;i<n;i++){
         a[i]=sc.nextInt();
     }
     for(int i=0;i<n;i++){
         for(int j=i+1;j<n;j++){
             if((a[i]*a[j])%3==0){
               
                 c++;
             }
         }
     }
       System.out.println(c);
    }
} 
