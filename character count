import java.util.*;
class Mddin{
    public static void main(String args[]){
    Scanner sc=new Scanner(System.in);
    String s=sc.nextLine();
    int n=sc.nextInt();
    int c=0;
    char ch=sc.next().charAt(0);
    for(int i=0; i<n ; i++){
        if(s.charAt(i)==ch){
            c++;
        }
    }
    System.out.println(c);
}
}
