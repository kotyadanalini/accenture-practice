import java.util.*;
class maini{
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int fi=fib(n)%47;
      
       System.out.println(fi);
 }
 public static int fib(int n){
     if(n==0){
         return 1;
     }
     if(n==1){
         return 1;
     }
     return fib(n-1)*fib(n-1)+fib(n-2)*fib(n-2);
 } 
}
